{
	"name": "roo-code",
	"displayName": "%extension.displayName%",
	"description": "%extension.description%",
	"publisher": "ZentarAI",
	"version": "0.2.0",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.96.4",
		"node": "20.19.2"
	},
	"author": {
		"name": "Roo Code"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Zentar-Ai/roo-code"
	},
	"homepage": "https://zentar.ai",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"roocode",
		"roo-code",
		"roo",
		"code",
		"cloud"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "roo-code-ActivityBar",
					"title": "%views.activitybar.title%",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"roo-code-ActivityBar": [
				{
					"type": "webview",
					"id": "roo-code.SidebarProvider",
					"name": "%views.sidebar.name%"
				}
			]
		},
		"commands": [
			{
				"command": "roo-code.plusButtonClicked",
				"title": "%command.newTask.title%",
				"icon": "$(add)"
			},
			{
				"command": "roo-code.promptsButtonClicked",
				"title": "%command.prompts.title%",
				"icon": "$(organization)"
			},
			{
				"command": "roo-code.mcpButtonClicked",
				"title": "%command.mcpServers.title%",
				"icon": "$(server)"
			},
			{
				"command": "roo-code.historyButtonClicked",
				"title": "%command.history.title%",
				"icon": "$(history)"
			},
			{
				"command": "roo-code.marketplaceButtonClicked",
				"title": "%command.marketplace.title%",
				"icon": "$(extensions)"
			},
			{
				"command": "roo-code.popoutButtonClicked",
				"title": "%command.openInEditor.title%",
				"icon": "$(link-external)"
			},
			{
				"command": "roo-code.accountButtonClicked",
				"title": "Account",
				"icon": "$(account)"
			},
			{
				"command": "roo-code.settingsButtonClicked",
				"title": "%command.settings.title%",
				"icon": "$(settings-gear)"
			},
			{
				"command": "roo-code.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-code.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-code.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "roo-code.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.importSettings",
				"title": "%command.importSettings.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.focusInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "roo-code.runSubagentTests",
				"title": "Run Subagent Tests",
				"category": "%configuration.title%"
			},
			{
				"command": "debugging-zentara-code.runFindUsagesTest",
				"title": "Zentara: Run findUsages Test",
				"category": "Zentara E2E Tests"
			},
			{
				"command": "zentara.runAllLspTests",
				"title": "Run All LSP Integration Tests",
				"category": "Zentara LSP Tests"
			},
			{
				"command": "zentara.runSpecificLspTest",
				"title": "Run Specific LSP Test",
				"category": "Zentara LSP Tests"
			},
			{
				"command": "zentara.showLspTestStatus",
				"title": "Show LSP Test Status",
				"category": "Zentara LSP Tests"
			},
			{
				"command": "zentara.runLspIntegrationTests",
				"title": "Run LSP Tool Integration Tests (via lspTool)",
				"category": "Zentara LSP Tests"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "roo-code.contextMenu",
					"group": "1"
				}
			],
			"roo-code.contextMenu": [
				{
					"command": "roo-code.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roo-code.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "roo-code.improveCode",
					"group": "1_actions@3"
				}
			],
			"terminal/context": [
				{
					"submenu": "roo-code.terminalMenu",
					"group": "2"
				}
			],
			"roo-code.terminalMenu": [
				{
					"command": "roo-code.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "roo-code.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "roo-code.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "roo-code.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.marketplaceButtonClicked",
					"group": "navigation@2",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.settingsButtonClicked",
					"group": "navigation@3",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.accountButtonClicked",
					"group": "navigation@4",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.historyButtonClicked",
					"group": "overflow@1",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.promptsButtonClicked",
					"group": "overflow@2",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.mcpButtonClicked",
					"group": "overflow@3",
					"when": "view == roo-code.SidebarProvider"
				},
				{
					"command": "roo-code.popoutButtonClicked",
					"group": "overflow@4",
					"when": "view == roo-code.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "roo-code.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.marketplaceButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.settingsButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.accountButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.historyButtonClicked",
					"group": "overflow@1",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.promptsButtonClicked",
					"group": "overflow@2",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.mcpButtonClicked",
					"group": "overflow@3",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				},
				{
					"command": "roo-code.popoutButtonClicked",
					"group": "overflow@4",
					"when": "activeWebviewPanelId == roo-code.TabPanelProvider"
				}
			]
		},
		"submenus": [
			{
				"id": "roo-code.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "roo-code.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"roo-code.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"roo-code.deniedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.deniedCommands.description%"
				},
				"roo-code.commandExecutionTimeout": {
					"type": "number",
					"default": 0,
					"minimum": 0,
					"maximum": 600,
					"description": "%commands.commandExecutionTimeout.description%"
				},
				"roo-code.commandTimeoutAllowlist": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.commandTimeoutAllowlist.description%"
				},
				"roo-code.preventCompletionWithOpenTodos": {
					"type": "boolean",
					"default": false,
					"description": "%commands.preventCompletionWithOpenTodos.description%"
				},
				"roo-code.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"roo-code.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				},
				"roo-code.enableCodeActions": {
					"type": "boolean",
					"default": true,
					"description": "%settings.enableCodeActions.description%"
				},
				"roo-code.autoImportSettingsPath": {
					"type": "string",
					"default": "",
					"description": "%settings.autoImportSettingsPath.description%"
				},
				"roo-code.useAgentRules": {
					"type": "boolean",
					"default": true,
					"description": "%settings.useAgentRules.description%"
				},
				"roo-cline.apiRequestTimeout": {
					"type": "number",
					"default": 600,
					"minimum": 0,
					"maximum": 3600,
					"description": "%settings.apiRequestTimeout.description%"
				},
				"roo-cline.newTaskRequireTodos": {
					"type": "boolean",
					"default": false,
					"description": "%settings.newTaskRequireTodos.description%"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint . --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"pretest": "turbo run bundle --cwd ..",
		"test": "jest -w=40% --verbose && vitest run",
		"format": "prettier --write .",
		"bundle": "node esbuild.mjs",
		"vscode:prepublish": "pnpm bundle --production",
		"vsix": "mkdirp ../bin && vsce package --no-dependencies --out ../bin",
		"publish:marketplace": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
		"watch:bundle": "pnpm bundle --watch",
		"watch:tsc": "cd .. && tsc --noEmit --watch --project src/tsconfig.json",
		"clean": "rimraf README.md CHANGELOG.md LICENSE dist logs mock .turbo"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"verbose": true,
		"setupFilesAfterEnv": [
			"<rootDir>/__mocks__/setup.ts"
		],
		"moduleNameMapper": {
			"^vscode$": "<rootDir>/__mocks__/vscode.ts"
		},
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"useESM": false
				}
			]
		},
		"testMatch": [
			"**/__tests__/**/*.test.ts",
			"**/?(*.)+(spec|test).ts"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json"
		],
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx}",
			"!src/**/*.d.ts",
			"!src/__mocks__/**",
			"!src/**/__tests__/**"
		],
		"testTimeout": 30000,
		"clearMocks": true,
		"resetMocks": true,
		"restoreMocks": true
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.848.0",
		"@aws-sdk/credential-providers": "^3.848.0",
		"@google/genai": "^1.0.0",
		"@lmstudio/sdk": "^1.1.1",
		"@mistralai/mistralai": "^1.9.18",
		"@modelcontextprotocol/sdk": "1.12.0",
		"@qdrant/js-client-rest": "^1.14.0",
		"@roo-code/cloud": "workspace:^",
		"@roo-code/ipc": "workspace:^",
		"@roo-code/telemetry": "workspace:^",
		"@roo-code/types": "workspace:^",
		"@vscode/codicons": "^0.0.36",
		"async-mutex": "^0.5.0",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"exceljs": "^4.4.0",
		"fast-deep-equal": "^3.1.3",
		"fast-glob": "^3.3.3",
		"fast-xml-parser": "^5.0.0",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
